{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nexport default {\n  data() {\n    return {\n      images: [],\n      selectedImages: [],\n      // Array to store selected image indices,\n      directoryarray: [`@/assets/1`, `@/assets/2`, `@/assets/3`]\n    };\n  },\n  mounted() {\n    this.loadImagesFromDirectory();\n  },\n  methods: {\n    loadImagesFromDirectory() {\n      // Generate a random directory number (1, 2, or 3)\n\n      // Construct the base directory path\n      const directoryPath = directoryarray[Math.floor(Math.random() * 3)];\n\n      // Manually import images from the selected directory\n      const imageFiles = require.context(directoryPath, false, /\\.(png|jpe?g|gif|svg)$/);\n      const images = imageFiles.keys().map(key => imageFiles(key).default);\n      this.images = images;\n    },\n    toggleImage(index) {\n      // Toggle the selected state of the image at the given index\n      if (this.isSelected(index)) {\n        this.selectedImages = this.selectedImages.filter(i => i !== index);\n      } else {\n        this.selectedImages.push(index);\n      }\n    },\n    isSelected(index) {\n      // Check if the image at the given index is selected\n      return this.selectedImages.includes(index);\n    }\n  }\n};","map":{"version":3,"names":["data","images","selectedImages","directoryarray","mounted","loadImagesFromDirectory","methods","directoryPath","Math","floor","random","imageFiles","require","context","keys","map","key","default","toggleImage","index","isSelected","filter","i","push","includes"],"sources":["C:\\Users\\Flemming Buol\\Desktop\\Work 2024\\Januar\\Captcha Errorama\\Errorama-Fail-to-be-human\\Lets Build it FR\\captchascheiterlauf\\src\\components\\Tilecaptcha.vue"],"sourcesContent":["<template>\r\n  <div class=\"burger\">\r\n    <div class=\"image-gallery\">\r\n      <img v-for=\"(image, index) in images\"\r\n           :key=\"index\"\r\n           :src=\"image\"\r\n           :alt=\"'Image ' + (index + 1)\"\