ction (d, i) {\r\n            return color(i / root.children.length);\r\n          });\r\n      }\r\n    },\r\n    mounted() {\r\n      this.drawBubbleChart(); // Call the method to draw bubble chart on component mount\r\n    }\r\n  };\r\n  </script>\r\n  \r\n  <style scoped>\r\n  .bubble {\r\n    font-family: \"Helvetica Neue\", Helvetica, Arial, sans-serif;\r\n    text-align: center;\r\n  }\r\n  .node circle {\r\n    fill: #ccc;\r\n    stroke: #fff;\r\n  }\r\n  </style>\r\n  "],"mappings":";AAYE,OAAO,KAAKA,EAAC,MAAO,IAAI;AAExB,eAAe;EACbC,IAAI,EAAE,YAAY;EAClBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,UAAU,EAAE;IACd,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACPC,eAAeA,CAAA,EAAG;MAChB;MACA,IAAI,CAACC,OAAO,CAACC,IAAI,CAAC,QAAQ,CAAC;IAC7B,CAAC;IACDC,eAAeA,CAAA,EAAG;MAChB;MACA,MAAMC,QAAO,GAAI,GAAG;MACpB,MAAMC,KAAI,GAAIV,EAAE,CAACW,eAAe,CAACX,EAAE,CAACY,kBAAkB,CAAC;MAEvD,MAAMC,MAAK,GAAIb,EAAE,CAACc,IAAI,CAAC,EACpBC,IAAI,CAAC,CAACN,QAAQ,EAAEA,QAAQ,CAAC,EACzBO,OAAO,CAAC,GAAG,CAAC;MAEf,MAAMC,GAAE,GAAIjB,EAAE,CAACkB,MAAM,CAAC,cAAc,EACjCC,MAAM,CAAC,KAAK,EACZC,IAAI,CAAC,OAAO,EAAEX,QAAQ,EACtBW,IAAI,CAAC,QAAQ,EAAEX,QAAQ,EACvBW,IAAI,CAAC,OAAO,EAAE,QAAQ,CAAC;MAE1B,MAAMC,KAAI,GAAIrB,EAAE,CAACsB,KAAK,CAAC,GAAG,EAAE,GAAG,CAAC,CAACC,GAAG,CAAC,OAAO;QAACC,MAAM,EAAEC,IAAI,CAACC,MAAM,CAAC,IAAI,EAAC,GAAI;MAAC,CAAC,CAAC,CAAC;MAE9E,MAAMC,IAAG,GAAI3B,EAAE,CAAC4B,SAAS,CAAC;QAACC,QAAQ,EAAER;MAAK,CAAC,EACxCS,GAAG,CAAC,UAAUC,CAAC,EAAE;QAAE,OAAOA,CAAC,CAACP,MAAM;MAAE,CAAC,CAAC;MAEzCX,MAAM,CAACc,IAAI,CAAC;MAEZ,MAAMK,IAAG,GAAIf,GAAG,CAACgB,SAAS,CAAC,OAAO,EAC/B/B,IAAI,CAACyB,IAAI,CAACE,QAAQ,EAClBK,KAAK,CAAC,CAAC,CAACf,MAAM,CAAC,GAAG,EAClBC,IAAI,CAAC,OAAO,EAAE,MAAM,EACpBA,IAAI,CAAC,WAAW,EAAE,UAAUW,CAAC,EAAE;QAC9B,OAAO,YAAW,GAAIA,CAAC,CAACI,CAAA,GAAI,GAAE,GAAIJ,CAAC,CAACK,CAAA,GAAI,GAAG;MAC7C,CAAC,CAAC;MAEJJ,IAAI,CAACb,MAAM,CAAC,QAAQ,EACjBC,IAAI,CAAC,GAAG,EAAE,UAAUW,CAAC,EAAE;QACtB,OAAOA,CAAC,CAACM,CAAC;MACZ,CAAC,EACAC,KAAK,CAAC,MAAM,EAAE,UAAUP,CAAC,EAAEQ,CAAC,EAAE;QAC7B,OAAO7B,KAAK,CAAC6B,CAAA,GAAIZ,IAAI,CAACE,QAAQ,CAAC