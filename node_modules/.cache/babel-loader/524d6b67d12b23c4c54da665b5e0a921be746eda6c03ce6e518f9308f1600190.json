   bubble(root);\r\n        const node = svg.selectAll(\".node\")\r\n          .data(root.children)\r\n          .enter().append(\"g\")\r\n          .attr(\"class\", \"node\")\r\n          .attr(\"transform\", function (d) {\r\n            return \"translate(\" + d.x + \",\" + d.y + \")\";\r\n          });\r\n  \r\n        node.append(\"circle\")\r\n          .attr(\"r\", function (d) {\r\n            return d.r;\r\n          })\r\n          .style(\"fill\", function (d) {\r\n            return color(d.Name);\r\n          });\r\n  \r\n        node.append(\"text\")\r\n          .attr(\"dy\", \".3em\")\r\n          .style(\"text-anchor\", \"middle\")\r\n          .text(function (d) {\r\n            return d.data.Name;\r\n          });\r\n      }\r\n    },\r\n    mounted() {\r\n      this.drawBubbleChart(); // Call the method to draw bubble chart on component mount\r\n    }\r\n  };\r\n  </script>\r\n  \r\n  <style scoped>\r\n  .bubble {\r\n    font-family: \"Helvetica Neue\", Helvetica, Arial, sans-serif;\r\n    text-align: center;\r\n  }\r\n  .node circle {\r\n    fill: #ccc;\r\n    stroke: #fff;\r\n  }\r\n  .node text {\r\n    font: 12px sans-serif;\r\n  }\r\n  </style>\r\n  "],"mappings":";AAYE,OAAO,KAAKA,EAAC,MAAO,IAAI;AAExB,eAAe;EACbC,IAAI,EAAE,YAAY;EAClBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,UAAU,EAAE;IACd,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACPC,eAAeA,CAAA,EAAG;MAChB;MACA,IAAI,CAACC,OAAO,CAACC,IAAI,CAAC,QAAQ,CAAC;IAC7B,CAAC;IACDC,eAAeA,CAAA,EAAG;MAChB;MACA,MAAMC,QAAO,GAAI,GAAG;MACpB,MAAMC,KAAI,GAAIV,EAAE,CAACW,YAAY,CAACX,EAAE,CAACY,gBAAgB,CAAC;MAElD,MAAMC,MAAK,GAAIb,EAAE,CAACc,IAAI,CAAC,EACpBC,IAAI,CAAC,CAACN,QAAQ,EAAEA,QAAQ,CAAC,EACzBO,OAAO,CAAC,GAAG,CAAC;MAEf,MAAMC,GAAE,GAAIjB,EAAE,CAACkB,MAAM,CAAC,cAAc,EACjCC,MAAM,CAAC,KAAK,EACZC,IAAI,CAAC,OAAO,EAAEX,QAAQ,EACtBW,IAAI,CAAC,QAAQ,EAAEX,QAAQ,EACvBW,IAAI,CAAC,OAAO,EAAE,QAAQ,CAAC;MAE1B,MAAMC,IAAG,GAAIrB,EAAE,CAACsB,SAAS,CAAC;QACtBC,QAAQ,EAAE,CAAC;UACTC,IAAI,EAAE,GAAG;UACTC,KAAK,EAAE;QACT,CAAC,EAAE;UACDD,IAAI,EAAE,GAAG;UACTC