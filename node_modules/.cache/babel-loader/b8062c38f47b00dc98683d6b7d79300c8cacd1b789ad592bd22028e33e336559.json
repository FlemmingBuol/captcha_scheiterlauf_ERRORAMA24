{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nexport default {\n  data() {\n    return {\n      images: []\n    };\n  },\n  mounted() {\n    this.loadImagesFromDirectory();\n  },\n  methods: {\n    loadImagesFromDirectory() {\n      const directoryPath = '@/assets/1'; // Replace this with the path to your directory\n      this.images = [];\n      // Assuming you have access to the File object here, you can loop through the files in the directory\n      // This example assumes you're using the FileReader API to read local files\n      const fileInput = document.createElement('input');\n      fileInput.type = 'file';\n      fileInput.multiple = true;\n      fileInput.directory = true;\n      fileInput.webkitdirectory = true;\n      fileInput.addEventListener('change', () => {\n        const files = Array.from(fileInput.files);\n        files.forEach(file => {\n          if (file.type.match('image.*')) {\n            const reader = new FileReader();\n            reader.onload = event => {\n              this.images.push(event.target.result);\n            };\n            reader.readAsDataURL(file);\n          }\n        });\n      });\n      fileInput.click();\n    }\n  }\n};","map":{"version":3,"names":["data","images","mounted","loadImagesFromDirectory","methods","directoryPath","fileInput","document","createElement","type","multiple","directory","webkitdirectory","addEventListener","files","Array","from","forEach","file","match","reader","FileReader","onload","event","push","target","result","readAsDataURL","click"],"sources":["C:\\Users\\Flemming Buol\\Desktop\\Work 2024\\Januar\\Captcha Errorama\\Errorama-Fail-to-be-human\\Lets Build it FR\\captchascheiterlauf\\src\\components\\Tilecaptcha.vue"],"sourcesContent":["<template>\r\n  <div class=\"image-gallery\">\r\n    <img v-for=\"(image, index) in images\" :key=\"index\" :src=\"image\" :alt=\"'Image ' + (index + 1)\" />\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  data() {\